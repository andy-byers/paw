// misc.paw

struct Struct<T> {v: T}
enum Enum<T> {X(T)}

pub fn recur_with_objects(n: int) {
    let list = [1, 2, 3];
    let tuple = ('a', 2, 3.0);
    let map = [1: 'a', 2: 'b', 3: 'c'];
    let struct_ = Struct{v: true};
    let enum_ = Enum::<str>::X('abc');
    let next_n = || n - 1;
    if n > 0 {
        recur_with_objects(next_n());
    }
}

pub fn test_gc() {
    recur_with_objects(100);
}
